<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		id="resultDialog"
		icon="sap-icon://message-success"
		title="{i18n>resultDialogTitle}">
		<content>
			<Text text="{/result}" class="sapUiSmallMargin resultColor" />
		</content>
		<beginButton>
			<Button
				id="suckit"
				text="{i18n>dialogCloseButtonText}"
				press=".onCloseDialog"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>
