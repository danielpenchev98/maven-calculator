<mvc:View
        xmlns="sap.m"
        xmlns:mvc="sap.ui.core.mvc">
    <Table
        id="calculationList"
        class="sapUiResponsiveMargin"
        width="auto">
        <!--items="{
             path : 'calculation>Calculations'
        }"-->
        <headerToolbar>
            <Toolbar>
                <Title text="{i18n>calculationListTitle}"/>
            </Toolbar>
        </headerToolbar>
        <columns>
            <Column id="equation">
                <Text text="{i18n>columnEquation}"/>
            </Column>
            <Column id="result">
               <Text text="{i18n>columnResult}"/>
            </Column>
        </columns>
    </Table>
</mvc:View>