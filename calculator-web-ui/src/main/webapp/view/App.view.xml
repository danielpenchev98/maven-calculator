<mvc:View
	controllerName="com.calculator.web.ui.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">
	<Shell>
		<App class="myAppDemoWT">
			<pages>
				<Page title="{i18n>homePageTitle}">
					<content>
						<mvc:XMLView viewName="com.calculator.web.ui.view.IntroductionPanel"/>
						<mvc:XMLView viewName="com.calculator.web.ui.view.RulesPanel"/>
						<mvc:XMLView viewName="com.calculator.web.ui.view.CalculatorPanel"/>
						<mvc:XMLView viewName="com.calculator.web.ui.view.CalculationHistory"/>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
