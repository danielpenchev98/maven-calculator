<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		id="resultDialog"
		title="Calculation result">
		<content>
			<Text text="{/result}"/>
		</content>
		<beginButton>
			<Button
				text="{i18n>dialogCloseButtonText}"
				press=".onCloseDialog"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>
