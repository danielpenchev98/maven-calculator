<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE dataset [
        <!ELEMENT dataset (calculator_responses*)>
        <!ELEMENT calculator_responses EMPTY>
        <!ATTLIST calculator_responses equation CDATA #REQUIRED>
        <!ATTLIST calculator_responses calculationResult CDATA #REQUIRED>
        <!ATTLIST calculator_responses errorMsg CDATA #IMPLIED>
        ]>
<dataset>
    <calculator_responses equation="1+1" calculationResult="2.0"/>
    <calculator_responses equation="1/0" calculationResult="0.0"/>
    <calculator_responses equation="2+1)" calculationResult="0.0" errorMsg="Missing or misplaced brackets"/>
    <calculator_responses equation="1++2"  calculationResult="0.0" errorMsg="Sequential components of the same type"/>
    <calculator_responses equation=" " calculationResult="0.0" errorMsg="Empty equation"/>
</dataset>

